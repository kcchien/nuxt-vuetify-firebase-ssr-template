{
  "name": "nuxt-vuetify-firebase-ssr-template",
  "version": "1.0.0",
  "description": "Nuxt.js, Vuefify, Firebase SSR template",
  "author": "Kuang-Cheng, Chien <jason@kingsteel.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:firebase": "npm run clean && npm run build && npm run copy && cd \"functions\" && npm i",
    "start": "nuxt start",
    "start:firebase": "firebase serve --only functions,hosting",
    "generate": "nuxt generate",
    "deploy": "firebase deploy --only functions,hosting",
    "copy": "cpx \"functions/.nuxt/dist/*\" \"public/_nuxt\" && cpx \"src/static/*.!(md)\" \"public\"",
    "clean": "npm run clean:public && npm run clean:functions && npm run clean:static",
    "clean:public": "rimraf \"public/*.!(md)\" && rimraf \"public/_nuxt\"",
    "clean:functions": "rimraf \"functions/node_modules\" && rimraf \"functions/.nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/browserconfig": "0.0.8",
    "@nuxtjs/component-cache": "^1.1.2",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^2.5.0",
    "@nuxtjs/sitemap": "^0.1.1",
    "@nuxtjs/vuetify": "^0.4.2",
    "babel-runtime": "^6.26.0",
    "nuxt": "^1.4.2",
    "vuetify": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "cpx": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-vue": "^4.7.1",
    "node-sass": "^4.9.3",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
